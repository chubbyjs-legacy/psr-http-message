{
  "name": "@chubbyjs/psr-http-message",
  "version": "1.1.2",
  "description": "Common Http-message Interface (PHP FIG PSR-7).",
  "keywords": [
    "psr",
    "psr-7",
    "http-message-interface"
  ],
  "author": "Dominik Zogg",
  "license": "MIT",
  "repository": "chubbyjs/psr-http-message",
  "scripts": {
    "cs-fix": "prettier --write src",
    "cs": "prettier --check src",
    "build": "tsc",
    "prepare": "rm -Rf dist && npm run build && npm run cs"
  },
  "prettier": {
    "printWidth": 120,
    "tabWidth": 4,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=12"
  },
  "devDependencies": {
    "@types/node": "12",
    "prettier": "2.1.2",
    "typescript": "^4.0.5"
  },
  "publishConfig": {
    "access": "public"
  }
}
